{
    "version": 2,
    "builds": [
        {
            "src": "server/server.js",
            "use": "@vercel/node",
            "config": {
                "distDir": "server"
            }
        }
    ],
    "routes": [
        {
            "src": "/{.*}",
            "dest": "/"
        }
    ]

}